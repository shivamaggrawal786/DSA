import java.util.Scanner;
public class Main
{
    static int countDigits(int n){
        int c = 0;
        while(n!=0){
            n=n/10;
            c+=1;
        }
        return c;
    }
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int digits = countDigits(n);
		System.out.println(digits);
	}
}
