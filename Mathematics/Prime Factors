import java.util.Scanner;
public class Main
{
    static void primeFactors(int n){
        if(n>1){
            i=2;
            while(n>1){
                if(n%i==0){
                    System.out.print(i+" ");
                    n/=i;
                }
                else{
                    i+=1;
                }
            }
        }
    }
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		primeFactors(n);
	}
}
