import java.util.Scanner;
public class Main
{
    static boolean isPrime(int n){
        int f = 0;
        for(int i=2;i<n;i++){
            if(n%i==0){
                return false;
            }
        }
        return true;
    }
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		boolean result = isPrime(n);
		System.out.println(result);
	}
}
